{
  "extends": ["react-app", "react-app/jest", "plugin:prettier/recommended"],
  "rules": {
    "prettier/prettier": "error",

    "react/react-in-jsx-scope": "off",
    "react/jsx-uses-react": "off",

    "no-console": ["warn", { "allow": ["warn", "error"] }],

    "linebreak-style": ["error", "unix"]
  }
}
